{
  "name": "pili_frontend",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "node-sass": "^4.5.0",
    "react-scripts": "0.9.5"
  },
  "dependencies": {
    "bulma": "^0.4.0",
    "react": "^15.4.2",
    "react-apollo": "^1.0.0-rc.3",
    "react-dom": "^15.4.2",
    "react-router-dom": "^4.0.0"
  },
  "scripts": {
    "build-css": "node-sass --include-path node_modules/bulma -o src/ src/ ",
    "watch-css": "npm run build-css && node-sass --include-path node_modules/bulma -o src/ src/ --watch",
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  }
}
